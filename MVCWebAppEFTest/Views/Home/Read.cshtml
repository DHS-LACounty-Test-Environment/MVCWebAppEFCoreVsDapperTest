@model Visitor[]


@for(int i=0; i< Model.Length; i++)
{
    <div class="card my-2">
        <div class="card-body">
            <div>
                @Html.LabelFor(m=>m[i].VisitorId) : @Html.DisplayFor(m => m[i].VisitorId)
            </div>
            <div>
                @Html.LabelFor(m => m[i].FirstName) : @Html.DisplayFor(m => m[i].FirstName)
            </div>
            <div>
                @Html.LabelFor(m => m[i].LastName) : @Html.DisplayFor(m => m[i].LastName)
            </div>
            <div>
                @Html.LabelFor(m => m[i].Age) : @Html.DisplayFor(m => m[i].Age)
            </div>
            <div>
                @Html.LabelFor(m => m[i].IsMinor) : @Html.DisplayFor(m => m[i].IsMinor)
            </div>
        </div>
    </div>
    
}
